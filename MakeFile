.PHONY: make_dataset
make_dataset:
	cat /mnt/c/Users/EverOrlandoReyesRuiz/Developer/rips-processor/assets/logs/errors.log | \
	grep $(LOG_PERIOD_VAR) | \
	grep -v "UTF-8" | grep -v "unicode code" | grep -v "invalid type: null" | \
	sed 's/\\/\//g' | \
	tr -d '\000' | \
	bash extract_keys.sh >> json_errors.csv
